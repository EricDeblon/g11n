msgstr "		%s\n\n"
msgid	"LINK EDITOR MEMORY MAP"
msgstr "連結編輯器記憶體對應"
#
# TRANSLATION_NOTE -- Entry map header
#
# The next message is a format string for a title. The title is composed of
# two lines. In C locale, it would look like:
#
# 	output		input		new
#	section		section		displacement	size
#
# The \t characters are used for alignment.  (output section), (input section),
# and (new displacement) have to be aligned.
#
msgid	"\n%s\t\t%s\t\t%s\n%s\t\t%s\t\t%s\t%s\n\n"
msgstr "\n%s		%s		%s\n%s		%s		%s	%s\n\n"
msgid	"\n%s\t\t%s\t\t%s\n%s\t\t%s\t\t%s\t\t%s\n\n"
msgstr "\n%s		%s		%s\n%s		%s		%s		%s\n\n"
msgid	"output"
msgstr "輸出"
msgid	"input"
msgstr "輸入"
msgid	"new"
msgstr "新"
msgid	"section"
msgstr "區段"
msgid	"displacement"
msgstr "代替"
msgid	"size"
msgstr "大小"
msgid	"virtual"
msgstr "虛擬"
msgid	"address"
msgstr "位址"
msgid	"%-8.8s\t\t\t%08.2llx\t%08.2llx\n"
msgstr "%-8.8s			%08.2llx	%08.2llx\n"
msgid	"\t\t%-8.8s\t%08.2llx\t%08.2llx %s\n"
msgstr "		%-8.8s	%08.2llx	%08.2llx %s\n"
