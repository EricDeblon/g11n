include $(SRC)/Makefile.master

LOCALE:sh = basename `pwd`

FILES = admin core_packages.txt gen_configfile.pl retrievecd.pl retrievecombined.pl Locale_config_S11x.txt Locale_config_S11s.txt

TARGETDIR = $(FILEROOT)/usr/sadm/lib/localeadm

TARGETDIRS = $(TARGETDIR)

SUBDIRS =  

all:=		TARGET= all
install :=	TARGET= install
clean :=	TARGET= clean
objclean := TARGET = objclean
clobber :=	TARGET= clobber

all: $(TARGETDIRS)

include $(SRC)/src/Makefile.targ

install: all $(FILES.obj) $(SUBDIRS)

clean: objclean $(SUBDIRS)
	$(RM) -r $(TARGETDIR)

FRC:
