msgstr "    fsexam [VOLBY] ... [soubor]\n"

#: src/fsexam.c:679
#, c-format
msgid ""
"\n"
"Supported options:\n"
msgstr ""
"\n"
"Podporované možnosti:\n"

#: src/fsexam.c:681
#, c-format
msgid "    -a, --auto-detect      Enable encoding auto detection\n"
msgstr "   -a, --auto-detect      Povolí autodetekci kódování\n"

#: src/fsexam.c:683
#, c-format
msgid "    -d dry-run-result      Specify the dryrun result file\n"
msgstr "    -d dry-run-result      Specifikuje soubor s dryrun výsledkem\n"

#: src/fsexam.c:684
#, c-format
msgid "    -E module-name         Enable special file handling\n"
msgstr "    -E module-name         Povolí speciální ukazatel na soubor\n"

#: src/fsexam.c:685
#, c-format
msgid "    -e encoding-list       Specify additional encoding list\n"
msgstr "    -e encoding-list       Specifikuje dodatečný seznam kódování\n"

#: src/fsexam.c:686
#, c-format
msgid "    -F, --force-convert    Forceful conversion mode\n"
msgstr "    -F, --force-convert    Silný mód konverze\n"

#: src/fsexam.c:687
#, c-format
msgid "    -f 'expression'        Specify file filter criteria\n"
msgstr "    -f 'expression'        Specifikuje kritéria filtrace\n"

#: src/fsexam.c:688
#, c-format
msgid "    -H, --hidden           Turn on hidden file handling\n"
msgstr "    -H, --hidden           Zapne ukazatel na skryté soubory\n"

#: src/fsexam.c:689
#, c-format
msgid "    -b, --auto-conversion  Interactive mode\n"
msgstr "    -b, --auto-conversion  Interaktivní mód\n"

#: src/fsexam.c:690
#, c-format
msgid "    -l, list-encoding      List all supported encoding\n"
msgstr "    -l, list-encoding      Seznam všech podporovaných kódování\n"

#: src/fsexam.c:691
#, c-format
msgid "    -k, --no-check-symlink-content\n"
msgstr "    -k, --no-check-symlink-content\n"

#: src/fsexam.c:692
#, c-format
msgid ""
"                           Don't check the consistence between\n"
"                           symbolic link and its target name\n"
msgstr ""
"                           Nekontroluje konzistenci mezi\n"
"                           symbolickým odkazem a jeho cílem\n"

#: src/fsexam.c:694
#, c-format
msgid "    -L logfile             Specify log file\n"
msgstr "    -L logfile             Specifikuje soubor s logy\n"

#: src/fsexam.c:696
#, c-format
msgid "    -n, --dry-run          Dryrun mode\n"
msgstr "    -n, --dry-run          Dryrun mód\n"

#: src/fsexam.c:697
#, c-format
msgid "    -P, --append-encoding-list\n"
msgstr "    -P, --append-encoding-list\n"

#: src/fsexam.c:698
#, c-format
msgid ""
"                           Append encoding list specified by\n"
"                           '-e' to predefined encoding list\n"
msgstr ""
"                           Připojí předdefinovaný seznam kódovaní specifikovaný\n"
"                           volbou '-e' \n"

#: src/fsexam.c:700
#, c-format
msgid "    -p, --prepend-encoding-list\n"
msgstr "    -p, --prepend-encoding-list\n"

#: src/fsexam.c:701
#, c-format
msgid ""
 "                           Prepend encoding list specified by\n"
"                           '-e' to predefined encoding list\n"                         
msgstr ""
"                           Odpojí předdefinovaný seznam kódovaní specifikovaný\n"
"                           volbou '-e' \n"

#: src/fsexam.c:703
#, c-format
msgid "    -R, --recursive        Recursive mode\n"
msgstr "    -R, --recursive        Rekurzivní mód\n"

#: src/fsexam.c:704
#, c-format
msgid "    -r, --remote           Turn on nfs files handling\n"
msgstr "    -r, --remote           Zapne ukazatel na nfs soubory\n"

#: src/fsexam.c:705
#, c-format
msgid "    -S, --save-encoding-list\n"
msgstr "    -S, --save-encoding-list\n"

#: src/fsexam.c:706
#, c-format
msgid ""
"                           Save encoding list specified by\n"
"                           '-e' permanently\n"
msgstr ""
"                           Uloží seznam kódovaní specifikovaný\n"
"                           volbou '-e' \n"
#: src/fsexam.c:708
#, c-format
msgid ""
"    -s, --restore          Restore the original name or\n"
"                           content for given files\n"
msgstr ""
"    -s, --restore          Obnoví původní jména nebo\n"
"                           obsah zadaných souborů\n"

#: src/fsexam.c:710
#, c-format
msgid ""
"    -t, --conv-content     Convert file content rather than\n"
"                           file name\n"
msgstr ""
"    -t, --conv-content     Zkonvertuje lépe obsah souboru\n"
"                           než jméno\n"
#: src/fsexam.c:712
#, c-format
msgid "    -w, --follow           Follow symbolic link\n"
msgstr "    -w, --follow           Následuje symbolické odkazy\n"

#: src/fsexam.c:713
#, c-format
msgid "    -V, --version          Print the version information\n"
msgstr "    -V, --version          Zobrazí informace o verzi\n"

#: src/fsexam.c:714
#, c-format
msgid "    -?, --help             Print this usage information for fsexam\n"
msgstr "    -?, --help             Zobrazí informace o použití pro fsexam\n"

