#
#	LC_TIME
#
LC_TIME

#
abday		"<U65E5>";"<U6708>";"<U706B>";"<U6C34>";\
		"<U6728>";"<U91D1>";"<U571F>"

#
day		"<U65E5><U66DC><U65E5>";"<U6708><U66DC><U65E5>";\
		"<U706B><U66DC><U65E5>";"<U6C34><U66DC><U65E5>";\
		"<U6728><U66DC><U65E5>";"<U91D1><U66DC><U65E5>";\
		"<U571F><U66DC><U65E5>"

#
abmon		"<space><one><U6708>";"<space><two><U6708>";\
		"<space><three><U6708>";"<space><four><U6708>";\
		"<space><five><U6708>";"<space><six><U6708>";\
		"<space><seven><U6708>";"<space><eight><U6708>";\
		"<space><nine><U6708>";"<one><zero><U6708>";\
		"<one><one><U6708>";"<one><two><U6708>"

#
mon		"<one><U6708>";"<two><U6708>";\
		"<three><U6708>";"<four><U6708>";\
		"<five><U6708>";"<six><U6708>";\
		"<seven><U6708>";"<eight><U6708>";\
		"<nine><U6708>";"<one><zero><U6708>";\
		"<one><one><U6708>";"<one><two><U6708>"

