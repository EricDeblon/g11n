	TurkeyQ6.kt UK6.kt US6.kt Japan7.kt Slovenia6.kt Slovakia6.kt \
	SerbiaandMontenegro6.kt Iceland6.kt Croatia6.kt Bulgaria6.kt \
	Belarus6.kt Malta_US6.kt Malta_UK6.kt Albania6.kt Czech6.kt \
	Greece6.kt Hungary6.kt Latvia6.kt Lithuania6.kt Poland6.kt \
	Macedonia6.kt Canada_Fr6.kt Portugal_Br6.kt Romania6.kt \
	Canada_Bi6.kt Brazil6.kt



XCOMM Map files are installed on all platforms
MAP_FILES = keytable.map xkbtable.map

ALL_FILES = $(SPARC_KEYBOARDS) $(X86_KEYBOARDS) $(USB_KEYBOARDS) $(MAP_FILES) 

InstallMultiple($(ALL_FILES),KEYTABLEDIR)

