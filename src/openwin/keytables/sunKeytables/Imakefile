#ifdef KEYTABLEDIR
#ifdef SparcArchitecture
XCOMM Type 4 & 5 are installed on Sparc only since they cannot be connected
XCOMM to non-sparc systems.  See below for Type 6 (USB) which can be used
XCOMM on both sparc & intel. 
SPARC_KEYBOARDS = Arabic5.kt Belgian5.kt Canada4.kt Canada_Fr5.kt Canada_Fr5_Hobo.kt \
	Canada_Fr5_TBITS5.kt Czech5.kt Denmark4.kt Denmark5.kt \
	Denmark5_Hobo.kt France5.kt France5_Hobo.kt FranceBelg4.kt \
	Germany4.kt Germany5.kt Germany5_Hobo.kt Hungary5.kt \
	Italy4.kt Italy5.kt Italy5_Hobo.kt Japan4.kt Japan5.kt \
	Japan5_Hobo.kt Korea4.kt Korea5.kt Korea5_Hobo.kt Netherland4.kt \
	Netherland5.kt Netherland5_Hobo.kt Norway4.kt Norway5.kt \
	Norway5_Hobo.kt Poland5.kt Poland5_pr.kt Portugal4.kt Portugal5.kt \
	Portugal5_Hobo.kt Russia5.kt Spain5.kt Spain5_Hobo.kt SpainLatAm4.kt \
	Sweden5.kt Sweden5_Hobo.kt SwedenFin4.kt Switzer_Fr4.kt \
	Switzer_Fr5.kt Switzer_Fr5_Hobo.kt Switzer_Ge4.kt Switzer_Ge5.kt \
	Switzer_Ge5_Hobo.kt Taiwan4.kt Taiwan5.kt Taiwan5_Hobo.kt Turkey5.kt \
	TurkeyF5.kt TurkeyQ5.kt UK4.kt UK5.kt UK5_Hobo.kt US101A_PC.kt \
	US101A_Sun.kt US3.kt US4.kt US5.kt US5_Hobo.kt US_UNIX5.kt \
	US_UNIX5_Hobo.kt Greece5.kt Latvia5.kt Lithuania5.kt 
#endif
#ifdef i386Architecture
