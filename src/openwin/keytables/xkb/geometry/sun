                 { <KPEQ>, 9 }, <KPDV>, <KPMU>, <KPSU> 

            };
        };
        row {
            top= 39;
            keys { <FRNT>, <COPY>, 
                 { <TAB>, 9, shape="TABK" },
                   <AD01>, <AD02>, <AD03>, <AD04>, <AD05>, 
                   <AD06>, <AD07>, <AD08>, <AD09>, <AD10>, 
                   <AD11>, <AD12>, { <RTRN>, "RTRN" },
                 { <KP7>, 9 }, <KP8>, <KP9>, { <KPAD>, "KPAD" } 
