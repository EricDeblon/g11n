#define IHaveSubdirs

     TESTDATA = 
    DATAFILES = amiga ataritt dell everex fujitsu hp keytronic kinesis \
		microsoft nec northgate pc sony sun winbook $(TESTDATA)
      SUBDIRS = digital sgi

all::

MakeXkbDir($(LIBDIR)/xkb,geometry)
InstallMultiple($(DATAFILES),$(LIBDIR)/xkb/geometry)
DependTarget()
