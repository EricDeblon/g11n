     DATAFILES = us

all::

install:: $(DATAFILES)
	@if [ -d $(DESTDIR)$(LIBDIR)/xkb/symbols/sun ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LIBDIR)/xkb/symbols/sun); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(DATAFILES); do \
	(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) $$i $(DESTDIR)$(LIBDIR)/xkb/symbols/sun); \
	done

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC); $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

