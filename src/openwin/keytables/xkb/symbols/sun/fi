
xkb_symbols "basic" {
    include "fi"
    // A few keys are different
    key <AE11> {	[            plus,        question      ],
			[	backslash			]	};
    key <AD12> {	[       diaeresis,     asciicircum     	],
			[      asciitilde			]	};
    key <BKSL> {	[      apostrophe,        asterisk	],
			[	quoteleft			]	};

};

