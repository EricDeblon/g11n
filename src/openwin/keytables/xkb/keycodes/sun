    <PROP> = 122;
    <UNDO> = 126;
    <FRNT> = 123;
    <COPY> = 128;
    <OPEN> = 120;
    <PAST> = 129;
    <FIND> = 130;
    <CUT>  = 127;
    <MUTE> = 131;
    <VOL-> = 133;
    <VOL+> = 132;
    <POWR> = 106;
    <COMP> = 105;
    alias <ALGR> = <RALT>;

    <LMTA> = 231;
    <RMTA> = 235;
