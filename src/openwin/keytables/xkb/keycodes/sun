xkb_keycodes "type6_jp_usb" {
    include "sun(type6_usb)"
    <EXEC> = 143;		// Kakutei/Muhenkan key
    <KANJ> = 142;		// Henkan key
    <HENK> = 140;		// Nihongo on-off/Hiragana_katakana key
    <UNDR> = 139;		// backslash key
    <BKSL> =  54;		// bracketright key
    <AE13> = 141;		// Yen key
    indicator 5 = "Kana";
};

