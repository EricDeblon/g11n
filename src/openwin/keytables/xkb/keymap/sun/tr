xkb_keymap "Turkey5" {
    xkb_keycodes	{ include "sun(type5tuv)"	};
    xkb_types		{ include "default"		};
    xkb_compatibility	{ include "default"		};
    xkb_symbols		{ include "sun/tr(type5)"	};
    xkb_geometry	{ include "sun(type5tuv)"	};
};

