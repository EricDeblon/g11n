	side		GL:Default
	length		1
	wc_encoding	\x00000000
	ct_encoding	ISO8859-1:GL
}

# 	cs1 class
cs1	{
	side		GLGR:Default
	length		2
	wc_encoding	\x30000000
	ct_encoding	gbk-0:GLGR
}

END XLC_XLOCALE
