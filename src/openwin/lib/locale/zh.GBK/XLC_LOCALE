# 
# 	XLC_FONTSET category
# 
XLC_FONTSET
# 	fs0 class (7 bit ASCII)
fs0	{
	charset		ISO8859-1:GL
	font		GBK1988.1989-0:GL;ISO8859-1:GL
}
# 	fs1 class
fs1	{
	charset		gbk-0:GLGR
	font {
		primary		gbk-0:GLGR,[\x8140,\xA0FE]
		supplement sunudczh.GBK.1997-0:GLGR,[\xA140,\xA1A0]->\x2121,\
				 	   [\xA240,\xA2A0]->\x2221,\
				 	   [\xA340,\xA3A0]->\x2321,\
				 	   [\xA440,\xA4A0]->\x2421,\
				 	   [\xA540,\xA5A0]->\x2521,\
				 	   [\xA640,\xA6A0]->\x2621,\
				 	   [\xA740,\xA7A0]->\x2721,\
				 	   [\xAAA1,\xAAFE]->\x2821,\
					   [\xABA1,\xABFE]->\x2921,\
					   [\xACA1,\xACFE]->\x2A21,\
					   [\xADA1,\xADFE]->\x2B21,\
					   [\xAEA1,\xAEFE]->\x2C21,\
					   [\xAFA1,\xAFFE]->\x2D21,\
					   [\xF8A1,\xF8FE]->\x2E21,\
					   [\xF9A1,\xF9FE]->\x2F21,\
					   [\xFAA1,\xFAFE]->\x3021,\
					   [\xFBA1,\xFBFE]->\x3121,\
					   [\xFCA1,\xFCFE]->\x3221,\
					   [\xFDA1,\xFDFE]->\x3321,\
					   [\xFEA1,\xFEFE]->\x3421
	}
}

END XLC_FONTSET

