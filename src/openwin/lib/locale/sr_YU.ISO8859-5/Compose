<C>		: "\306"	Cyrillic_TSE
<V>		: "\262"	Cyrillic_VE
<B>		:  "\261"	Cyrillic_BE
<N>		: "\275"	Cyrillic_EN
<M>		: "\274"	Cyrillic_EM




END_STATE

