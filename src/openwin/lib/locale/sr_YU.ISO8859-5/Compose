# Compose table for the Serbian language [gelf]
#


#
# Native English mode
#

STATE   "[ English ]"
Ctrl<space>    :  SWITCH_STATE_TO "[ Serbian ]"
Ctrl<T>    : SWITCH_STATE_TO  "[ Serbian ]"
END_STATE


#
# Serbian Mode
#


STATE   "[ Serbian ]"
Ctrl<T>    :  SWITCH_STATE_TO "[ English ]"
Ctrl<space>    :  SWITCH_STATE_TO "[ English ]"
#
#

# Second row
# Comment: numerosign is a Cyrillic keysym

#Not Needed
#<at>            : "\042"	quotedbl
#<numbersign>    : "\360"	numerosign
#<dollar>        : "\045"	percent
#<percent>       : "\072"	colon
#<asciicircum>   : "\054"	comma
#<ampersand>     : "\056"	period
#<asterisk>      : "\073"	semicolon
#<numerosign>    : "\360"	numerosign


# Third row


<q>		: "\371"	Cyrillic_lje
<w>		: "\372"	Cyrillic_nje	
<e>		: "\325"	Cyrillic_ie
<r>		: "\340"	Cyrillic_er
<t>		: "\342"	Cyrillic_te
<y>		: "\327"	Cyrillic_ze
<u>		: "\343"	Cyrillic_u
<i>		: "\330"	Cyrillic_i
<o>		: "\336"	Cyrillic_o
<p>		: "\337"	Cyrillic_pe
<bracketleft>	: "\350"	Cyrillic_sha
<bracketright>	: "\362"	Serbian_dje	

<Q>		: "\251"	Cyrillic_LJE	
<W>		: "\252"	Cyrillic_NJE	
<E>		: "\265"	Cyrillic_IE
<R>		: "\300"	Cyrillic_ER
<T>		: "\302"	Cyrillic_TE
<Y>		: "\267"	Cyrillic_ZE
<U>		: "\303"	Cyrillic_U
<I>		: "\270"	Cyrillic_I
<O>		: "\276"	Cyrillic_O
<P>		: "\277"	Cyrillic_PE
Lock<bracketleft> 	: "\310"	Cyrillic_SHA
Lock<bracketright>	: "\242"	Serbian_DJE
<braceleft> 	: "\310"	Cyrillic_SHA
<braceright>	: "\242"	Serbian_DJE	


# Fourth row

<a>		: "\320"	Cyrillic_a
<s>		: "\341"	Cyrillic_es
<d>		: "\324"	Cyrillic_de
<f>		: "\344"	Cyrillic_ef
<g>		: "\323"	Cyrillic_ghe
<h>		 : "\345"	Cyrillic_ha
<j>		: "\370"	Cyrillic_je	
<k>		: "\332"	Cyrillic_ka
<l>		: "\333"	Cyrillic_el
<semicolon>	: "\347"	Cyrillic_che
<quoteright>	: "\373"	Serbian_tshe	

<A>		: "\260"	Cyrillic_A
<S>		: "\301"	Cyrillic_ES
<D>		: "\264"	Cyrillic_DE
<F>		: "\304"	Cyrillic_EF
<G>		: "\263"	Cyrillic_GHE
<H>		: "\305"	Cyrillic_HA
<J>		: "\250"	Cyrillic_JE		
<K>		 : "\272"	Cyrillic_KA
<L>		: "\273"	Cyrillic_EL
<colon>		: "\307"	Cyrillic_CHE
<quotedbl>	: "\253"	Serbian_TSHE 		


# Fifth row

<z>		: "\365"	Serbian_dze	
