# 
# 	XLC_FONTSET category
# 
XLC_FONTSET
# 	fs0 class 
fs0	{
	charset		ISO8859-1:GL
	font		ISO8859-1:GL
}
# 	fs1 class 
fs1	{
	charset		koi8-r:GR
	font		koi8-r:GR
}
END XLC_FONTSET

# 
# 	XLC_XLOCALE category
# 
XLC_XLOCALE

encoding_name		koi8-r
mb_cur_max		1
state_depend_encoding	False

# 	cs0 class
cs0	{
	side		GL:Default
	length		1
	ct_encoding	ISO8859-1:GL
}


# 	cs1 class
cs1	{
	side		GR:Default
	length		1
	ct_encoding	koi8-r:GR
}

END XLC_XLOCALE
