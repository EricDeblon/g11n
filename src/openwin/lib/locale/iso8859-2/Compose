<iacute>				: "\355"	iacute	                
<eacute>				: "\351"	eacute	                
<uacute>				: "\372"	uacute	                
<uring>					: "\371"	uring	                
<section>				: "\247"	section	                          
<ssharp>				: "\337"	ssharp

