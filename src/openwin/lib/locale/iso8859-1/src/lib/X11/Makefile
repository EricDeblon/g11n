all::

includes:: $(HEADERS)
	@if [ -d $(BUILDINCDIR)/. ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(BUILDINCDIR)/.); fi
	@(set -x; cd $(BUILDINCDIR)/.; for i in $(HEADERS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)/./$(CURRENT_DIR)/$$i .; \
	done)

install:: $(HEADERS)
	@if [ -d $(DESTDIR)$(INCDIR)/X11 ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)/X11); fi
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTINCFLAGS) $$i $(DESTDIR)$(INCDIR)/X11); \
	done

