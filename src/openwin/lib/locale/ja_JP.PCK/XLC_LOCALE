# 
# 	XLC_FONTSET category
# 
XLC_FONTSET
# 	fs0 class (7 bit ASCII)
fs0	{
	#charset	ISO8859-1:GL
	#font		ISO8859-1:GL; JISX0201.1976-0:GL
	charset		ISO8859-1:GL
	font		JISX0201.1976-0:GL; ISO8859-1:GL
}
# 	fs1 class (Kanji)
fs1	{
	charset		JISX0208.1983-0:GL
	font {
		primary		JISX0208.1983-0:GL,[\x2121,\x747e]
		supplement	sunudcja.1997-0:GL,[\x7521,\x7e7e]->\x2121
	}
}
# 	fs2 class (Half Kana)
fs2	{
	charset		JISX0201.1976-0:GR
	font		JISX0201.1976-0:GR
}
# 	fs3 class (User Defined Character and IBM Extended Character)
fs3	{
	charset		SUNUDCJA.1997-0
	font {
		load_option	delay_nocheck
		primary		JISX0212.1990-0:GL,[\x2121,\x747e]
		supplement	sunudcja.1997-0:GL,[\x7521,\x7e7e]->\x2b21
	}
}
END XLC_FONTSET

# 
# 	XLC_XLOCALE category
# 
XLC_XLOCALE

encoding_name		ja_JP.PCK
mb_cur_max		2
state_depend_encoding	False

# 	cs0 class
cs0	{
	side		GL:Default
	length		1
	ct_encoding	ISO8859-1:GL; JISX0201.1976-0:GL
}

# 	cs1 class
cs1	{
	side		GR:Default
	length		2
	ct_encoding	JISX0208.1983-0:GL; JISX0208.1983-0:GR;\
			JISX0208.1983-1:GL; JISX0208.1983-1:GR
}

# 	cs2 class
cs2	{
	side		GR
	length		1
	ct_encoding	JISX0201.1976-0:GR
}

# 	cs3 class
cs3	{
	side		GL
	length		2
	ct_encoding	JISX0212.1990-0:GL ; JISX0212.1990-0:GR
}

# 	cs4 class
cs4	{
	side		GR
	length		2
	ct_encoding	SUNUDCJA.1997-0
}

END XLC_XLOCALE

