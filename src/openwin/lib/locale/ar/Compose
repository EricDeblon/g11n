#
# Arabic input table (Ascii input mode)
#
# Note: The quoted string after the token STATE will appear 
# in the status area when XIM status area is specified.

STATE   "[ English ]"
Ctrl<T>     : SWITCH_STATE_TO  "[ Arabic ]"
Ctrl<space> : SWITCH_STATE_TO  "[ Arabic ]"
<EuroSign> : "e"

END_STATE


#
# Arabic input table (Arabic input mode)
#


STATE   "[ Arabic ]"
Ctrl<T>      :  SWITCH_STATE_TO "[ English ]"
Ctrl<space>  :  SWITCH_STATE_TO "[ English ]"

#
#
# <A> not verified
# A, C, J, K, S, T, U, V, X, Z, {, }, <, > changed/added
 
<A>		: "\xf0"
# <B> => <lam> <alef>
<B>		: "\xe4\xc2"
# <C> => <kasratan> \xed
<C>		: "\x7b"
#<D> => opening square bracket
<D>		: "["
<E>		: "\xef"
#<F> => closing square bracke
<F>		: "]"
#<G> => <lam> <hamzaonalef> 
<G>		: "\xe4\xc3"
<J>		: "\xe0"
<H>		: "\xc3"
<I>		: "\xe7"
<K>		: "\xac"
<L>		: "/"
<M>		: "\xc9"
<N>		: "\xc2"
<O>		: "\xce"
<P>		: "\xbb"
<Q>		: "\xee"
<R>		: "\xec"
<S>		: "\xed"
<T>		: "\xe4\xc5"
<U>		: "\xac"
#<V> = <kasra>                         \xf0
<V>		: "\x7d"
<W>		: "\xeb"
<X>		: "\xf2"
<Y>		: "\xc5"
<Z>		: "\xc6"
<a>		: "\xd4"
# <b> => <lam> <maddaonalef>
<b>		: "\xe4\xc7"
<c>		: "\xc4"
<d>		: "\xea"
<e>		: "\xcb"
<f>		: "\xc8"
<g>		: "\xe4"
<h>		: "\xc7"
<i>		: "\xe7"
<j>		: "\xca"
<k>		: "\xe6"
<l>		: "\xe5"
<m>		: "\xc9"
<n>		: "\xe9"
<o>		: "\xce"
<p>		: "\xcd"
<q>		: "\xd6"
<r>		: "\xe2"
<s>		: "\xd3"
<t>		: "\xe1"
<u>		: "\xd9"
<v>		: "\xd1"
<w>		: "\xd5"
<x>		: "\xc1"
<y>		: "\xda"
<z>		: "\xc6"
<apostrophe>	: "\xd7"
#<plus>		: "\xab"
<comma>		: "\xe8"
#<minus>		: "\xad"
<period>	: "\xd2"
<slash>		: "\xd8"
<semicolon>	: "\xe3"
#<equal>		: "\xbd"
<greater>	: "\x2e" 	
<less>       	: "\x2c"	
<question>	: "\xbf"
#<backslash>	: "\xe0"
#ok
<bracketleft>	: "\xcc"
<bracketright>	: "\xcf"
#<underscore>	: "\xdf"
#ok
<grave>		: "\xd0"
#<bar>		: "\xfc"
#ok
<asciitilde>	: "\xf1"
<braceleft>		: "\x3e"
<braceright>		: "\x3c"



END_STATE

