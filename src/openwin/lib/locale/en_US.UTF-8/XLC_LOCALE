	}
}
#
# The last fallback class. This must appear as the last one.
fs17	{
	charset	UNICODE-FONTSPECIFIC:GLGR
	font	{
	load_option	delay_nocheck
		primary	UNICODE-FONTSPECIFIC:GLGR
	}
}

END	XLC_FONTSET

